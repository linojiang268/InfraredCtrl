<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:infraredctrl="http://schemas.android.com/apk/res/com.infraredctrl.activity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_listview_header"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="@drawable/set_navbar"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/set_imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/left"
            android:clickable="false" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:src="@drawable/left" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1.0px"
        android:background="@drawable/task_list_line" />

    <ScrollView
        android:id="@+id/sSetScrollView"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:fadingEdge="none" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="vertical" >

            <View
                android:layout_width="match_parent"
                android:layout_height="1.0px"
                android:layout_marginTop="30dp"
                android:background="@drawable/task_list_line" />

            <LinearLayout
                android:id="@+id/llSetDevice"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/item_pressed_click_style"
                android:orientation="horizontal"
                android:paddingBottom="12dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:paddingTop="12dp" >

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/set_cofig1" />

                <TextView
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1.0"
                    android:gravity="left|center_vertical"
                    android:text="@string/setting_up_tw_deploy_facility"
                    android:textColor="#000000"
                    android:textSize="17sp" />

                <ImageView
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/ic_arrow_black" />
                <!-- @drawable/abc_ic_go_search_api_holo_light -->
                <!-- android:src="@drawable/ic_arrow_black" -->
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1.0px"
                android:background="@drawable/task_list_line" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1.0px"
                android:layout_marginTop="30dp"
                android:background="@drawable/task_list_line" />

            <LinearLayout
                android:id="@+id/llDataShare"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/item_pressed_click_style"
                android:orientation="horizontal"
                android:paddingBottom="12dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:paddingTop="12dp" >

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/set_data_share1" />

                <TextView
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1.0"
                    android:gravity="left|center_vertical"
                    android:text="@string/setting_up_tw_data_share"
                    android:textColor="#000000"
                    android:textSize="17sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF" >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1.0px"
                    android:layout_marginLeft="45dp"
                    android:background="@drawable/task_list_line" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llDataGet"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/item_pressed_click_style"
                android:orientation="horizontal"
                android:paddingBottom="12dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:paddingTop="12dp" >

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/set_data_share1" />

                <TextView
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1.0"
                    android:gravity="left|center_vertical"
                    android:text="@string/setting_up_tw_data_get"
                    android:textColor="#000000"
                    android:textSize="17sp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1.0px"
                android:background="@drawable/task_list_line" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1.0px"
                android:layout_marginTop="30dp"
                android:background="@drawable/task_list_line" />

            <LinearLayout
                android:id="@+id/llClickVibration"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/item_pressed_click_style"
                android:orientation="horizontal"
                android:paddingBottom="8dp"
                android:paddingLeft="10dp"
                android:paddingRight="5dp"
                android:paddingTop="8dp"
                android:visibility="visible" >

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/set_key_shake1" />

                <TextView
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1.0"
                    android:gravity="left|center_vertical"
                    android:text="@string/setting_up_tw_key_shake"
                    android:textColor="#000000"
                    android:textSize="17sp" />

                <CheckBox
                    android:id="@+id/cbClickVibration"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/ibt_set_time"
                    android:button="@null" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF" >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1.0px"
                    android:layout_marginLeft="45dp"
                    android:background="@drawable/task_list_line" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llClickSound"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/item_pressed_click_style"
                android:orientation="horizontal"
                android:paddingBottom="8dp"
                android:paddingLeft="10dp"
                android:paddingRight="5dp"
                android:paddingTop="8dp"
                android:visibility="visible" >

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/set_key_sound1" />

                <TextView
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1.0"
                    android:gravity="left|center_vertical"
                    android:text="@string/setting_up_tw_key_sound"
                    android:textColor="#000000"
                    android:textSize="17sp" />

                <CheckBox
                    android:id="@+id/cbClickSound"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/ibt_set_time"
                    android:button="@null" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF" >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1.0px"
                    android:layout_marginLeft="45dp"
                    android:background="@drawable/task_list_line" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llDownloadUrl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/item_pressed_click_style"
                android:orientation="horizontal"
                android:paddingBottom="12dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:paddingTop="12dp"
                android:visibility="visible" >

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/set_app_dowload_link1" />

                <TextView
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1.0"
                    android:gravity="left|center_vertical"
                    android:text="@string/setting_up_tw_app_dowload_link"
                    android:textColor="#000000"
                    android:textSize="17sp" />

                <ImageView
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/ic_arrow_black" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF" >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1.0px"
                    android:layout_marginLeft="45dp"
                    android:background="@drawable/task_list_line" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llCommonProblem"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/item_pressed_click_style"
                android:orientation="horizontal"
                android:paddingBottom="12dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:paddingTop="12dp"
                android:visibility="visible" >

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/set_often_matter1" />

                <TextView
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1.0"
                    android:gravity="left|center_vertical"
                    android:text="@string/setting_up_tw_often_matter"
                    android:textColor="#000000"
                    android:textSize="17sp" />

                <ImageView
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/ic_arrow_black" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF" >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1.0px"
                    android:layout_marginLeft="45dp"
                    android:background="@drawable/task_list_line" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llUserNoteBook"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/item_pressed_click_style"
                android:orientation="horizontal"
                android:paddingBottom="12dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:paddingTop="12dp"
                android:visibility="visible" >

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/set_user_notebook1" />

                <TextView
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1.0"
                    android:gravity="left|center_vertical"
                    android:text="@string/setting_up_tw_user_notebook"
                    android:textColor="#000000"
                    android:textSize="17sp" />

                <ImageView
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/ic_arrow_black" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1.0px"
                android:background="@drawable/task_list_line" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1.0px"
                android:layout_marginTop="30dp"
                android:background="@drawable/task_list_line" />

            <LinearLayout
                android:id="@+id/llOneKeyDel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/item_pressed_click_style"
                android:orientation="horizontal"
                android:paddingBottom="12dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:paddingTop="12dp" >

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/set_del1" />

                <TextView
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1.0"
                    android:gravity="left|center_vertical"
                    android:text="@string/setting_up_tw_one_key_del"
                    android:textColor="#000000"
                    android:textSize="17sp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1.0px"
                android:background="@drawable/task_list_line" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1.0px"
                android:layout_marginTop="30dp"
                android:background="@drawable/task_list_line" />

            <LinearLayout
                android:id="@+id/llAboutYunduo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/item_pressed_click_style"
                android:orientation="horizontal"
                android:paddingBottom="12dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:paddingTop="12dp" >

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:contentDescription="@null"
                    android:src="@drawable/set_user_about_yunduo_icon" />

                <TextView
                    android:layout_width="0.0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1.0"
                    android:gravity="left|center_vertical"
                    android:text="@string/setting_up_tw_about_yunduo"
                    android:textColor="#000000"
                    android:textSize="17sp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1.0px"
                android:layout_marginBottom="30dp"
                android:background="@drawable/task_list_line" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>